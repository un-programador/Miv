{
  "manifest_version": 2,
  "name": "Miv",
  "version": "1.7",

  "description": "Some vim like keymappings for Firefox and Chromium browsers.",

  "icons": {
    "48": "icons/icon48.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["miv.js"]
    }
  ],

  "browser_specific_settings": {
    "gecko": {
      "id": "minivim@mathiashh.dk"
    }
  }
}


